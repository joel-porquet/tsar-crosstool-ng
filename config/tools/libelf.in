# libelf config file

config TOOL_libelf
    help
      libelf lets you read, modify or create ELF files in an
      architecture-independent way.

choice
    bool
    prompt "libelf version"

config LIBELF_V_0_8_10
    bool
    prompt "0.8.10"

config LIBELF_V_0_8_11
    bool
    prompt "0.8.11"

# CT_INSERT_VERSION_ABOVE
